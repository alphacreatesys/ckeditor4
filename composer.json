{
    "name": "alphacreatesys/ckeditor4",
    "description": "Integrate CKEDITOR into laravel-admin",
    "type": "library",
    "keywords": ["laravel-admin", "extension", "editor", "CKEditor"],
    "homepage": "https://github.com/alphacreatesys/ckeditor4",
    "license": "MIT",
    "authors": [
        {
            "name": "alphacreatesys",
            "email": "okamoto@alpha-create.ne.jp"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "encore/laravel-admin": "~1.6"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "Encore\\CKEditor\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Encore\\CKEditor\\CKEditorServiceProvider"
            ]

        }
    }
}
